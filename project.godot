config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "VirtualJoystick",
"language": "GDScript",
"path": "res://joystick/virtual_joystick.gd"
} ]
_global_script_class_icons={
"VirtualJoystick": ""
}

[application]

config/name="Basic 3d person controller with  touchpad support"
run/main_scene="res://src/gui/start_screen.tscn"
config/icon="res://icon.png"

[autoload]

Music="*res://src/music/music.tscn"
Globalsettings="*res://src/global_settings.gd"
Save="*res://src/save/save.gd"

